    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SuperMercado/bin/Debug/netcoreapp3.1/SuperMercado.dll",
            "args": [],
            "cwd": "${workspaceFolder}/SuperMercado",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Debugger App Arreglo",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Ejemplo-Arreglo-como-Atributo/bin/Debug/netcoreapp3.1/Ejemplo-Arreglo-como-Atributo.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Ejemplo-Arreglo-como-Atributo",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}